{
  "author": "github-actions[bot] <41898282+github-actions[bot]@users.noreply.github.com>",
  "plugins": [
    "git-tag",
    [
      "exec",
      {
        "getPreviousVersion": "echo AFTER_VERSION $ARG_0 arg1 $ARG_1",
        "afterChangelog": "VERSION=$(~/auto version);VERSION=${VERSION#'v'}; bumpver update --set-version $VERSION -n"
      }
    ]
  ]
}


